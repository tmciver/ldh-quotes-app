PREFIX dct:     <http://purl.org/dc/terms/>
PREFIX schema:  <https://schema.org/>
PREFIX rdfs:    <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd:     <http://www.w3.org/2001/XMLSchema#>

CONSTRUCT
{
    ?this dct:title [ a xsd:string ] ;
        schema:text [ a xsd:string ] ;
        schema:spokenByCharacter [ ?speaker a schema:Person ] ;
        schema:isBasedOn [ a rdfs:Resource ] ;
        schema:datePublished [ a xsd:date ] .
    ?speaker schema:name ?speakerName .
}
WHERE {}
